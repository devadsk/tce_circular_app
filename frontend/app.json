{
  "expo": {
    "name": "TCE Circular App",
    "slug": "tce_circular_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tcelogo.png",
    "scheme": "tce-circular-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tcelogo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,

      /** ðŸ”¥ Important â€” Your own package name */
      "package": "com.jenissajiya.tcecircularapp"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/tcelogo.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tcelogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        /** ðŸ”¥ Your own EAS project ID */
        "projectId": "25dd07c8-0777-4cf4-9e45-748f0b764fc8"
      }
    },

    /** ðŸ”¥ Your Expo username */
    "owner": "jenissajiya"
  }
}

